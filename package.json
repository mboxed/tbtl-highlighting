{
    "name": "tbtl-highlighting",
    "displayName": "TBTL Highlighting",
    "description": "Highlighting for the Tablatal format",
    "version": "1.0.0",
    "engines": {
        "vscode": "^1.34.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "tbtl",
            "aliases": ["Tablatal", "tbtl"],
            "extensions": [".tbtl"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "tbtl",
            "scopeName": "source.tbtl",
            "path": "./syntaxes/tbtl.tmLanguage"
        }]
    }
}